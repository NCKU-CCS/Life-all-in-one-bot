import json
import csv
import psycopg2

connection_string = "dbname='bot_db' user='bot_user' password='bot'"

connection = psycopg2.connect(connection_string)
cursor = connection.cursor()

#reader = csv.reader(open('joke.json','rb'))
with open('/home/mini/djangogirls/Life-all-in-one-bot/bot/fb/joke.json') as data_file:
	json_data = json.load(data_file)

for element in json_data:
	print(element)
	statement = "INSERT INTO Joke (title,context) VALUES ('" + element['title'] + "," + ['context'] + ")"

	cursor.execute(statement)
	connection.commit()
	templateid = cursor.fetchone()[0]


